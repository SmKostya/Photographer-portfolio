block main
    .back
        div
            span Прошлый вид фотосессий
        div
            span Выйти с галереи
        div
            span Следующий вид фотосессий
    .gallery
        .swiper-container.main-slider.loading
            .swiper-wrapper
                - for (var i = 1; i < 10; i++)
                    .swiper-slide
                        figure.slide-bgimg(style="background-image:url(/img/types/wedding/"+i+".jpg)")
                            img.entity-img(src="/img/types/wedding/"+i+".jpg")
                        .content
                            p.title
                            span.caption
            .swiper-button-prev.swiper-button-white
            .swiper-button-next.swiper-button-white
        .swiper-container.nav-slider.loading
            .swiper-wrapper(role="navigation")
                - for (var i = 1; i < 10; i++)
                    .swiper-slide
                        figure.slide-bgimg(style="background-image:url(/img/types/wedding/"+i+".jpg)")
                            img.entity-img(src="/img/types/wedding/"+i+".jpg")
                        .content
                            p.title
    
    